{
  "extends": [
    "tslint:latest",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules",
      "build"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "prettier": [true, {
      "printWidth": 120,
      "singleQuote": true
    }],
    "no-console": false
  }
}
